#include <stdio.h>

int main() {

    int array[5];

    printf("Informe 5 valores:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &(*(array + i)));
    }

    printf("O dobro de cada valor:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", 2 * (*(array + i))); 
    }

    return 0;
}
